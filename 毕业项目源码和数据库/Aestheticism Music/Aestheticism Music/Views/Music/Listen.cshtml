@{
    Layout = null;
}

@using Aestheticism_Music.Models;
@{ 
    MusicList list = ViewBag.list;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge，chrome=1">
    <title>该页面正在播放- @list.MusicListName</title>

    <link href="~/Content/Music/css/style.css" rel="stylesheet" />
</head>
<body>
    <audio controls autoplay>
